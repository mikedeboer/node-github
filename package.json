{
  "name": "@octokit/rest",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "GitHub REST API client for Node.js",
  "keywords": [
    "octokit",
    "github",
    "rest",
    "api-client"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "contributors": [
    {
      "name": "Mike de Boer",
      "email": "info@mikedeboer.nl"
    },
    {
      "name": "Fabian Jakobs",
      "email": "fabian@c9.io"
    },
    {
      "name": "Joe Gallo",
      "email": "joe@brassafrax.com"
    },
    {
      "name": "Gregor Martynus",
      "url": "https://github.com/gr2m"
    }
  ],
  "repository": "https://github.com/octokit/rest.js",
  "dependencies": {
    "@octokit/core": "^2.4.0",
    "@octokit/plugin-paginate-rest": "^2.0.0",
    "@octokit/plugin-request-log": "^1.0.0",
    "@octokit/plugin-rest-endpoint-methods": "file:../plugin-rest-endpoint-methods.js/pkg"
  },
  "devDependencies": {
    "@octokit/auth": "^1.1.1",
    "@octokit/fixtures-server": "^6.0.0",
    "@octokit/request": "^5.2.0",
    "@types/node": "^13.1.0",
    "btoa-lite": "^1.0.0",
    "chai": "^4.1.2",
    "cypress": "^3.0.0",
    "http-proxy-agent": "^4.0.0",
    "lolex": "^5.1.2",
    "mkdirp": "^1.0.0",
    "mocha": "^7.0.1",
    "nock": "^11.3.3",
    "npm-run-all": "^4.1.2",
    "nyc": "^15.0.0",
    "prettier": "^1.14.2",
    "proxy": "^1.0.0",
    "semantic-release": "^17.0.0",
    "sinon": "^8.0.0",
    "sinon-chai": "^3.0.0",
    "string-to-arraybuffer": "^1.0.0",
    "typescript": "^3.3.1",
    "universal-user-agent": "^4.0.0"
  },
  "types": "index.d.ts",
  "scripts": {
    "coverage": "nyc report --reporter=html && open coverage/index.html",
    "lint": "prettier --check '{lib,test}/**/*.{js,json,ts}' 'docs/*.{js,json}' 'docs/src/**/*' index.js README.md package.json",
    "lint:fix": "prettier --write '{lib,test}/**/*.{js,json,ts}' 'docs/*.{js,json}' 'docs/src/**/*' index.js README.md package.json",
    "pretest": "npm run -s lint",
    "test": "nyc mocha test/mocha-node-setup.js \"test/*/**/*-test.js\"",
    "test:browser": "cypress run --browser chrome",
    "start-fixtures-server": "octokit-fixtures-server"
  },
  "license": "MIT",
  "nyc": {
    "ignore": [
      "test"
    ]
  },
  "release": {
    "publish": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/github",
        "assets": [
          "dist/*",
          "!dist/*.map.gz"
        ]
      }
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/octokit-rest.min.js.gz",
      "maxSize": "33 kB"
    }
  ]
}
